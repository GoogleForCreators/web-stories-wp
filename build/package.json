{
  "name": "Web-Stories-WP-micro-site",
  "private": "true",
  "version": "0.0.3",
  "license": "MIT",
  "description": "A website!",
  "scripts": {
    "build": "npm run js-build && eleventy",
    "watch": "concurrently \"eleventy --serve\" \"npm run js-build-watch\" \"npm run test-watch\"",
    "serve": "eleventy --serve",
    "js-build": "rollup -c rollup.config.js",
    "js-build-watch": "rollup -c rollup.config.js -w",
    "debug": "DEBUG=* eleventy && npm run test",
    "test": "mocha test/test*.js",
    "test-watch": "mocha test/test*.js --watch"
  },
  "pre-push": [
    "build"
  ],
  "pre-commit": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/google/web-stories-wp.git"
  },
  "homepage": "https://wp.stories.google/",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.3",
    "eleventy-plugin-local-images": "^0.4.0",
    "file-type": "^12.0.1",
    "fs-extra": "^8.1.0",
    "jsdom": "^15.1.1",
    "luxon": "^1.21.3",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "node-fetch": "^2.6.1",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "rollup-plugin-terser": "^6.1.0",
    "shorthash": "0.0.2"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@ampproject/eleventy-plugin-amp": "^0.5.5",
    "@saschazar/wasm-avif": "^1.0.0",
    "clean-css": "^4.2.3",
    "concurrently": "^5.2.0",
    "csp-hash-generator": "0.0.3",
    "csso": "^4.0.3",
    "datauri": "^3.0.0",
    "expect.js": "^0.3.1",
    "hasha": "^5.2.0",
    "html-minifier": "^4.0.0",
    "image-size": "^0.8.3",
    "lru-cache": "^5.1.1",
    "mocha": "^8.1.3",
    "purge-from-html": "^1.0.3",
    "purgecss": "^2.3.0",
    "rollup": "^2.23.0",
    "sharp": "^0.25.4",
    "twitter": "^1.7.1"
  },
  "peerDependencies": {
    "nunjucks": "^3"
  }
}
