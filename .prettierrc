{
  "bracketSpacing": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "quoteProps": "preserve",
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "parser": "markdown"
      }
    },
    {
      "files": [
        ".eslintrc",
        ".prettierrc",
        ".stylelintrc",
        "*.json"
      ],
      "options": {
        "parser": "json"
      }
    },
    {
      "files": [
        "*.yml"
      ],
      "options": {
        "parser": "yaml"
      }
    },
    {
      "files": [
        "*.xml",
        "*.xml.dist"
      ],
      "options": {
        "parser": "xml"
      }
    }
  ]
}
