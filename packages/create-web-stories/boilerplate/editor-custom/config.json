    "@googleforcreators/story-editor",
    "@googleforcreators/elements",
    "@googleforcreators/element-library",
    "react-router-dom"
  ],
  "devDependencies": [],
  "replacements": [
    {
      "src": "gitignore",
      "dest": ".gitignore"
    },
    {
      "src": "webpack-configs",
      "dest": "webpack-configs"
    },
    {
      "src": ".babelrc",
