  "dependencies": [
    "@googleforcreators/story-editor",
    "@googleforcreators/dashboard",
    "@googleforcreators/elements",
    "@googleforcreators/element-library",
    "react-router-dom"
  ],
  "devDependencies": [],
  "replacements": [
    {
