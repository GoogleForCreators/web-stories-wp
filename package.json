{
  "name": "web-stories-wp",
  "title": "Web Stories",
  "private": true,
  "description": "Visual storytelling for WordPress.",
  "author": "Google",
  "license": "Apache-2.0",
  "keywords": [
    "amp",
    "stories",
    "storytelling",
    "wordpress"
  ],
  "homepage": "https://github.com/google/web-stories-wp",
  "repository": {
    "type": "git",
    "url": "https://github.com/google/web-stories-wp.git"
  },
  "bugs": {
    "url": "https://github.com/google/web-stories-wp/issues"
  },
  "dependencies": {
    "draft-js": "^0.11.4",
    "draft-js-export-html": "^1.4.1",
    "draft-js-import-html": "^1.4.1",
    "draftjs-filters": "^2.3.0",
    "prop-types": "^15.7.2",
    "react-modal": "^3.11.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@storybook/addon-a11y": "^5.3.9",
    "@storybook/addon-docs": "^5.3.9",
    "@storybook/addon-knobs": "^5.3.9",
    "@storybook/addon-storysource": "^5.3.9",
    "@storybook/addon-viewport": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "@testing-library/react-hooks": "^3.2.1",
    "@wordpress/api-fetch": "^3.9.0",
    "@wordpress/babel-preset-default": "^4.9.0",
    "@wordpress/components": "^9.0.0",
    "@wordpress/dom-ready": "^2.6.0",
    "@wordpress/editor": "^9.10.0",
    "@wordpress/element": "^2.10.0",
    "@wordpress/eslint-plugin": "^3.3.0",
    "@wordpress/i18n": "^3.8.0",
    "@wordpress/scripts": "^6.2.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "browserslist": "^4.8.5",
    "core-js": "^3.6.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-utils": "^2.0.0",
    "husky": "^4.2.1",
    "jest-silent-reporter": "^0.1.2",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-moveable": "^0.16.6",
    "styled-components": "^5.0.0",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "svg-inline-loader": "^0.8.0",
    "terser-webpack-plugin": "^2.3.2",
    "uuid": "^3.4.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpackbar": "^4.0.0"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:js": "wp-scripts build",
    "build:plugin": "wp dist-archive . $(pwd)/build/web-stories --create-target-dir",
    "download-fonts": "curl -s \"https://www.googleapis.com/webfonts/v1/webfonts?fields=items&prettyPrint=false&key=$GOOGLE_FONTS_API_KEY\" | jq -c '[.items[] | { family: .family, variants: .variants, category: .category }]' > includes/data/fonts.json",
    "dev": "wp-scripts start",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "wp-scripts lint-style \"assets/src/**/*.(js|css)\"",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:js": "wp-scripts lint-js",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:php": "vendor/bin/phpcs",
    "lint:php:fix": "vendor/bin/phpcbf",
    "build-storybook": "build-storybook -c .storybook -o build/storybook --quiet",
    "storybook": "start-storybook --quiet",
    "test": "npm-run-all --parallel test:*",
    "test:js": "wp-scripts test-unit-js --config=tests/js/jest.config.js",
    "test:js:help": "wp-scripts test-unit-js --help",
    "test:js:watch": "npm run test:js -- --watch",
    "test:php": "vendor/bin/phpunit",
    "test:php:help": "npm run test:php -- --help"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:js",
      "pre-push": "npm run test:js"
    }
  }
}
